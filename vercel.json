{
  "version": 2,
  "builds": [
    {
      "src": "server_vercel_demo.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server_vercel_demo.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
